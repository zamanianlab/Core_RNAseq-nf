repo="Core_RNAseq-nf"

executor {
    name   = 'local'
    cpus   = 24
    memory = '128GB'
    queueSize = 3
}

params {
   data="$PWD/data"
   output="$PWD/output"
   aux="$PWD/${repo}/auxillary"
   trace="$PWD/trace"

   big=8
   small=4
}

timeline {
  enabled = true
  file = "${params.trace}/execution_timeline.html"
}
report {
  enabled = true
  file = "${params.trace}/execution_report.html"
}
trace {
  enabled = true
  file = "${params.trace}/execution_trace.txt"
}
